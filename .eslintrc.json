{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "comma-dangle": 0,
    "prefer-const": 1,
    "no-const-assign": 1,
    "no-console": 0,
    "implicit-arrow-linebreak": "below"
  }
}
